<h1>Listing account_entries</h1>

<table>
  <tr>
    <th>Debit</th>
    <th>Credit</th>
    <th>Status</th>
    <th>Reason</th>
  </tr>

<% for account_entry in @account_entries %>
  <tr>
    <td><%=h account_entry.debit %></td>
    <td><%=h account_entry.credit %></td>
    <td><%=h account_entry.status %></td>
    <td><%=h account_entry.reason %></td>
    <td><%= link_to 'Show', account_entry %></td>
    <td><%= link_to 'Edit', edit_account_entry_path(account_entry) %></td>
    <td><%= link_to 'Destroy', account_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New account_entry', new_account_entry_path %>
